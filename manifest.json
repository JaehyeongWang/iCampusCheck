{
    "manifest_version": 2,
   
    "name": "iCampus Check",
    "description": "Show to-do list on SKKU iCampus",
    "version": "0.0.2",

    "content_scripts": [
      {
        "matches": [
          "*://canvas.skku.edu/*",
          "*://icampus.skku.edu/*"
        ],
        "js": ["content.js", "jquery-3.5.0.min.js"]
      }
    ],
   
    "browser_action": {
      "default_icon": "icon.png",
      "default_popup": "popup.html"
    },
    "icons": {
      "128": "icon.png"
    },
    "content_security_policy": "script-src 'self' https://canvas.skku.edu/; object-src 'self'",
    "permissions": [
      "activeTab", "tabs"
    ]
  }